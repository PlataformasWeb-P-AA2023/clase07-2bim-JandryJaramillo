      <br>\n                            <router-link :to=\"{name: 'edit', params: { id: estudiante.url }}\" class=\"btn btn-sm btn-primary\">Editar</router-link>\n                            <button class=\"btn btn-danger btn-sm ml-1\" v-on:click=\"deleteEstudiante(estudiante)\">Eliminar</button>\n                        </div>\n                    </div>\n                    <div class=\"col-md-8\">\n                        <div class=\"card-body\">\n                            <p class=\"card-text\">Ciudad: {{ estudiante.ciudad }}</p>\n                            <p class=\"card-text\">Tipo: {{ estudiante.tipo }}</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <p  v-if=\"estudiantes.length == 0\"> Sin Estudiantes</p>\n    </div>\n</template>\n<script>\n\nimport axios from 'axios';\n\nexport default {\n    data() {\n        return {\n            estudiantes: []\n        }\n    },\n    created() {\n        this.all();\n    },\n    methods: {\n        deleteEstudiante: function(e) {\n            if (confirm('Eliminar ' + e.nombre)) {\n                axios.delete(e.url)\n                    .then( response => {\n                        this.all();\n                    });\n            }\n        },\n        all: function () {\n            axios.get('http://127.0.0.1:8000/edificios/')\n                .then( response => {\n                    this.estudiantes = response.data                    \n                });\n        }\n    },\n}\n</script>\n"]}]}