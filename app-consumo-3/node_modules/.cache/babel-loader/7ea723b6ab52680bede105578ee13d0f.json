n                    return;\n                }\n                axios.put((this.$route.params.id),\n                        this.estudiante\n                    )\n                    .then(response => {\n                        this.$router.push('/');\n                    })\n            });\n        }\n    },\n}\n</script>\n"],"sourceRoot":"src/components"}]}